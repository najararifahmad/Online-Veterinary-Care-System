
@{
    ViewBag.Title = "Appointments";
}


<!-- Counts Section -->
<section class="dashboard-counts section-padding">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <a href="~/user/bookappointment">Book Appointment</a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <h2>Previously Booked Appointments</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <table class="table table-hover table-responsive">
                    <thead>
                        <tr>
                            <th>Token No</th>
                            <th>Doctor Name</th>
                            <th>Appointment Date</th>
                            <th>Booking Slot</th>
                            <th>#</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</section>

